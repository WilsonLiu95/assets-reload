{
  "name": "assets-realod",
  "version": "1.0.0",
  "description": "switch all assets cdn name (script,link,background-image,img). when you open one website in a test domain you can switch cdn name to location.hostname",
  "author": "wilsonliuxyz@gmail.com",
  "license": "MIT",
  "main": "dist/assets-realod.common.js",
  "module": "dist/assets-realod.esm.js",
  "unpkg": "dist/assets-realod.js",
  "jsdelivr": "dist/assets-realod.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:WilsonLiu95/assets-reload.git"
  },
  "files": [
    "src/*.js",
    "dist/*.js"
  ],
  "keywords": [
    "cdn",
    "name",
    "switch",
    "cdn",
    "switch",
    "test",
    "domain"
  ],
  "scripts": {
    "dev": "node examples/server.js",
    "dev:dist": "rollup -wm -c build/rollup.dev.config.js",
    "build": "node build/build.js",
    "lint": "eslint src examples",
    "test": "npm run lint &&  npm run test:unit",
    "test:unit": "jasmine JASMINE_CONFIG_PATH=test/unit/jasmine.json",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "changelog": "conventional-changelog -p angular -r 2 -i CHANGELOG.md -s",
    "release": "bash scripts/release.sh"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommitMsg.js"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow-vue": "^1.0.0",
    "buble": "^0.19.3",
    "camelize": "^1.0.0",
    "conventional-changelog-cli": "^2.0.11",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.10",
    "es6-promise": "^4.2.4",
    "eslint": "^4.19.1",
    "eslint-plugin-vue-libs": "^2.1.0",
    "express": "^4.16.2",
    "express-urlrewrite": "^1.2.0",
    "jasmine": "2.8.0",
    "lint-staged": "^8.1.0",
    "path-to-regexp": "^1.7.0",
    "rollup": "^0.56.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-watch": "^4.0.0",
    "uglify-js": "^3.3.13",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.1",
    "vue-template-compiler": "^2.5.16",
    "vuepress": "^0.14.1",
    "vuepress-theme-vue": "^1.1.0",
    "webpack": "^4.9.0",
    "webpack-dev-middleware": "^3.1.3",
    "yorkie": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/WilsonLiu95/assets-reload/issues"
  },
  "homepage": "https://github.com/WilsonLiu95/assets-reload#readme"
}
